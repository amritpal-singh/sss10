<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0.0"
        id          = "com.cisc.sssten"
        version     = "1.0.0" >

    <!-- versionCode is optional and Android only -->
    <preference name="phonegap-version" value="3.0.0" />
    <name>SSS10</name>

    <description>
        Permissible Structural Steel Seismic-Force-Resisting Systems in accordance with NBC 2010 and CSA S16-09.
    </description>

    <author href="https://build.phonegap.com" email="support@phonegap.com">
        
    </author>
    
    <icon src="res/android/icon-72-hdpi.png" />
    
    <feature name="SplashScreen">
      <param name="ios-package" value="CDVSplashScreen" />
    </feature>

    <gap:splash src="res/screen/ios/Default~iphone.png" gap:platform="ios" width="320" height="480" />
    <gap:splash src="res/screen/ios/Default@2x~iphone.png" gap:platform="ios" width="640" height="960" />
    <gap:splash src="res/screen/ios/Default-Portrait~ipad.png" gap:platform="ios" width="640" height="1136" />
    <gap:splash src="res/screen/ios/Default-Portrait.png" gap:platform="ios" width="768" height="1004" />
    <gap:splash src="res/screen/ios/Default-Landscape.png" gap:platform="ios" width="1024" height="768" />
    


    
</widget>